{
  "name" : "ivoglent/phpriak",
  "description" : "Riak driver for php",
  "type" : "library",
  "authors" : [{
    "name" : "Ivoglent Nguyen",
    "email" : "ivoglent@gmail.com"
  }],
  "keywords" : [
    "riak",
    "nosql",
    "riakcs"
  ],
  "license" : [
    "BSD-3-Clause"
  ],
  "require" : {
    "ext-curl" : "*",
    "php" : ">=5.4.0",
    "basho/riak" : "*"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*",
    "phpunit/php-invoker" : ">=1.1"
  },
  "autoload" : {
    "psr-4" : {
      "ivoglent\\phpriak\\" : "src"
    }
  },
  "minimum-stability" : "dev",
  "config" : {
    "vendor-dir" : "vendor"
  }
}
