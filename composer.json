{
  "name" : "ivoglent/yiiriak",
  "description" : "Riak driver for yii2",
  "type" : "yii2-extension",
  "authors" : [{
    "name" : "Ivoglent Nguyen",
    "email" : "ivoglent@gmail.com"
  }],
  "keywords" : [
    "yii2",
    "riak",
    "nosql",
    "riakcs"
  ],
  "license" : [
    "BSD-3-Clause"
  ],
  "require" : {
    "ext-curl" : "*",
    "php" : ">=5.4.0",
    "basho/riak" : "*"
  },
  "require-dev" : {
    "phpunit/phpunit" : "3.7.*",
    "phpunit/php-invoker" : ">=1.1",
    "yiisoft/yii2" : "dev-master"
  },
  "autoload" : {
    "psr-4" : {
      "ivoglent\\yiiriak\\" : "src"
    },
    "classmap": [
      "composer_packages/yiisoft/yii/framework/YiiBase.php",
      "composer_packages/yiisoft/yii/framework/yii.php"
    ]
  },
  "minimum-stability" : "dev",
  "config" : {
    "vendor-dir" : "vendor"
  }
}
